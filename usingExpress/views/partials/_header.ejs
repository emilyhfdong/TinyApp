<div class="jumbotron">
  <h1>TinyApp Project</h1>
  <% if (!username) {%>
  <form method="POST" action= "/login" class = "form-inline">
    <input id="username" type="text" name="username" placeholder="username" style="width: 300px">
    <input type="submit" value="Submit" class = "btn btn-primary btn-sm">
  </form>
  <%} else {%>
  <h6>Username: <%=username %> </h6>
  <form method="POST" action= "/logout">
    <input type="submit" value="Logout" class = "btn btn-primary btn-sm">
  </form>
  <%}%>

</div>

